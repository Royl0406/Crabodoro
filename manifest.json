{
    "name": "Crabodoro",
    "description": "Informs you what tab you're currently on",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": "Assets/hewwo.png",
        "default_popup": "src/Game-menus/main.html"
    },
    "permissions": ["tabs", "notifications", "scripting", "storage"],
    "host_permissions": ["https://www.youtube.com/*", "http://*/", "https://*/"],
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    }
}