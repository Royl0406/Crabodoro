{
    "name": "Crabodoro",
    "description": "Informs you what tab you're currently on",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "main.html"
    },
    "permissions": ["tabs", "scripting", "storage", "notifications"],
    "host_permissions": ["https://www.youtube.com/*", "http://*/", "https://*/"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "128": "128.png"
    }
}